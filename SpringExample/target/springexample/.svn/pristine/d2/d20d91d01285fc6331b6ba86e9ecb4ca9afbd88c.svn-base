			<forward name="list" path="/uis/video/recordList.jsp" />
			<forward name="previewlist" path="/uis/video/previewList.jsp" />
		</action>
		<action path="/videoXml" type="com.sohu.frontweb.uis.video.action.XmlAction" parameter="method">
