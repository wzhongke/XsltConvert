		<action path="/apollo" type="com.sohu.frontweb.apollo.action.ApolloAction" parameter="method">
			<forward name="basic" path="/apollo/basic.jsp" />
			<forward name="trend" path="/apollo/trend.jsp" />
			<forward name="flow" path="/apollo/flow.jsp" />
		</action>			
	
		<!--  合作方 -->
		<action path="/cooperation" type="com.sohu.frontweb.cooperation.action.CooperationAction" parameter="method">
			<forward name="list" path="/cooperation/index.jsp" />
