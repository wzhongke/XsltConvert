<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.sohu.frontweb.anchor.data.AnchorBlack" table="anchor_blacklist">
<id column="id" name="id" type="java.lang.Long">
<generator class="native"/>
</id>
<property column="url" length="255" name="url" not-null="true" type="java.lang.String"/>
<property column="weight" length="10" name="weight" type="java.lang.Integer"/>
<property column="start_time" length="19" name="startTime" type="java.util.Date"/>
<property column="submitter" length="128" name="submitter" not-null="true" type="java.lang.String"/>
<property column="create_time" length="19" name="createTime" not-null="true" type="java.util.Date"/>
<property column="modify_time" length="19" name="modifyTime" not-null="true" type="java.util.Date"/>
</class>
</hibernate-mapping>