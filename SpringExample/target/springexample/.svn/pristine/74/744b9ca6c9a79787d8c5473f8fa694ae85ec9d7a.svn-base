    	function batchSubmitForm(){
   				var recordIdList = getCheckedIdList();

				var msgTip = Ext.MessageBox.show({
					title:'提示',
					width : 250,
					msg:'正在修改表记录信息请稍后......'
				});
	    				
				Ext.Ajax.request({
					url : 'secadmin_output.do?method=batchModifyRecord',
					params : {recordIdList : recordIdList.toString(),sogouRank:document.getElementById("batchSogouRank").value,
					startDateStr:document.getElementById("batchStartDateStr").value,endDateStr:document.getElementById("batchEndDateStr").value
					},
					method : 'POST',
					success : function(response,options){
						msgTip.hide();
						var result = Ext.util.JSON.decode(response.responseText);
						if(result.success){
							//服务器端数据成功删除后，同步删除客户端列表中的数据
							batchWin.hide();
							store.load({params:{start:0, limit:18}});
							Ext.Msg.alert('提示','修改记录成功。');
						}else{
							Ext.Msg.alert('提示','修改记录失败!');
						}
					},
					failure : function(response,options){
						msgTip.hide();
						Ext.Msg.alert('提示','修改记录请求失败！');
					}
				});    				
    	}    	
        function batchNewSubmitForm(){

            var msgTip = Ext.MessageBox.show({
                title:'提示',
                width : 250,
                msg:'正在修改表记录信息请稍后......'
            });
                    
            Ext.Ajax.request({
                url : 'secadmin_output.do?method=batchNewModifyRecord',
                params : {recordIdList : document.getElementById("inputdata").value,sogouRank:document.getElementById("batchSogouRank").value,
                startDateStr:document.getElementById("batchStartDateStr1").value,endDateStr:document.getElementById("batchEndDateStr1").value
                },
                method : 'POST',
                success : function(response,options){
                    msgTip.hide();
                    var result = Ext.util.JSON.decode(response.responseText);
                    if(result.success){
                        //服务器端数据成功删除后，同步删除客户端列表中的数据
                        batchNewWin.hide();
                        store.load({params:{start:0, limit:18}});
                        Ext.Msg.alert('提示','修改记录成功。');
                    }else{
                        Ext.Msg.alert('提示','修改记录失败!');
                    }
                },
                failure : function(response,options){
                    msgTip.hide();
                    Ext.Msg.alert('提示','修改记录请求失败！');
                }
            });                 
    }
    	
